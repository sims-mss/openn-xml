              </respStmt>
              <respStmt>
                <resp>former owner</resp>
                <persName>Sneyd, Walter, 1809-1888</persName>
              </respStmt>
            </msItem>
            <msItem n="1r">
              <locus>1r</locus>
              <title>Statutes, f. 1r</title>
            </msItem>
            <msItem n="37v">
              <locus>37v</locus>
              <title>Section on Achaea, f. 37v</title>
            </msItem>
            <msItem n="39v">
              <locus>39v</locus>
              <title>List of obscure words, f. 39v</title>
            </msItem>
            <msItem n="40r">
